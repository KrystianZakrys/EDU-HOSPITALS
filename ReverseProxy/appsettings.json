{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://localhost:7077"
      }
    }
  },
  "AllowedHosts": "*",
  "ReverseProxy": {
    "Routes": {
      "clinics": {
        "ClusterId": "clinics",
        "Match": { "Path": "/clinics/{**catch-all}" }
      },
      "emergency": {
        "ClusterId": "emergency",
        "Match": { "Path": "/emergency/{**catch-all}" }
      },
      "hospitals": {
        "ClusterId": "hospitals",
        "Match": { "Path": "/hospitals/{**catch-all}" }
      },
      "patientcard": {
        "ClusterId": "patientcard",
        "Match": { "Path": "/patientcard/{**catch-all}" }
      }
    },
    "Clusters": {
      "clinics": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7067"
          }
        }
      },
      "emergency": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7053"
          }
        }
      },
      "hospitals": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7076"
          }
        }
      },
      "patientcard": {
        "Destinations": {
          "destination1": {
            "Address": "https://localhost:7110"
          }
        }
      }
    }
  }
}
